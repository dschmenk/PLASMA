cat rel/apple/DGRLIB#FE1000        | ./ac.jar -p   PLFORTH.PO sys/DGRLIB REL
cat scripts/plasma.4th             | ./ac.jar -ptx PLFORTH.PO sys/scripts/PLASMA.4TH TXT
