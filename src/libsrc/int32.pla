export def i32tos(i32ptr, strptr)#1
    res[t_i32] save
    word iptr, rem
    char[12] istr
    
    iptr = @istr.11
    store32(@save)
    load32(i32ptr)
    if i32ptr->3 & $80
        neg32()
        putc('-')
    fin
    repeat
        drop, rem = divi16(10) // Only care about LSW of remainder
        ^iptr = rem + '0'
        iptr--
    until iseqi16(0)
    ^iptr = @istr.11 - iptr
    strcpy(strptr, iptr)
    load32(@save)
    return strptr
end
export def puti32(i32ptr)#0
    char[12] i32str
        
    puts(i32tos(i32ptr, @i32str))
end

