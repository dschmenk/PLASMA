export asm iseqi16(imm16)#1
        LDY     #$00
        LDA     ESTKL+0,X       ; IMM16L
        STA     SRC+0
        LDA     ESTKH+0,X       ; IMM16H
        STA     SRC+1
        BPL     +               ; SIGN EXTEND
        DEY
+       STY     SRC+2
        STY     SRC+3
        LDY     #$00
_ISEQ   LDA     ACCUM32+0
        CMP     SRC+0
        BNE     +
        LDA     ACCUM32+1
        CMP     SRC+1
        BNE     +
        LDA     ACCUM32+2
        CMP     SRC+2
        BNE     +
        LDA     ACCUM32+3
        CMP     SRC+3
        BNE     +
        DEY
+       STY     ESTKL+0,X
        STY     ESTKH+0,X
        RTS
end
