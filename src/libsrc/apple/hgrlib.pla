export asm hgrColor(c)#0
        LDA     ESTKL+0,X   ; COLOR
        AND     #$07
        ASL
        TAY
        LDA     $1000,Y     ; HCOLORL
        STA     $2000       ; CURHCLRL
        LDA     $1001,Y     ; HCOLORH
        STA     $2001       ; CRHCLRH
        INX
        RTS
end
