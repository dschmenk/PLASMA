export asm hgrCopyDst(ofst, y, w, h, dstptr)#0
        JSR     $C000       ; CLIPSRC
;        LDA     ESTKL+0,X   ; DST PTR
        LDA     SRCL
        STA     DSTL
;        LDA     ESTKH+0,X
        LDA     SRCH
        STA     DSTH
--      LDY     ESTKL+3,X   ; Y COORD
        LDA     ESTKL+4,X   ; HORIZ OFFSET
        CLC
end
asm     _hgrCopyDst
        ADC     $8000,Y     ; HGRSCANL
        STA     SRCL
        LDA     $1000       ; DRAWBUFFH
        ADC     $9000,Y     ; HGRSCANH
        STA     SRCH
        LDY     ESTKH+2,X   ; WIDTH
        DEY
-       LDA     (SRC),Y
        STA     (DST),Y
        DEY
        BPL     -
        LDA     DSTL
        CLC
        ADC     ESTKL+2,X   ; PITCH
        STA     DSTL
        BCC     +
        INC     DSTH
+       INC     ESTKL+3,X   ; Y COORD
        DEC     ESTKL+1,X   ; HEIGHT
        BNE     --
        INX
        INX
        INX
        INX
        INX
        RTS
end
